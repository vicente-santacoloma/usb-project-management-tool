<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="proyectos.hibernate.ActividadArtefacto" table="ACTIVIDADARTEFACTO">
      <!-- 0 -->
      <id name="idActividadArtefacto" column="idActividadArtefacto">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <many-to-one name="actividad" column="actividad" />      <!-- 2 -->
      <many-to-one name="artefacto" column="artefacto" />
    </class>
</hibernate-mapping>